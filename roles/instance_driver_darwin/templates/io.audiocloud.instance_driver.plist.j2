<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>io.audiocloud.instance_driver</string>
        <key>LimitLoadToSessionType</key>
        <array>
            <string>Aqua</string>
            <string>Background</string>
            <string>LoginWindow</string>
            <string>StandardIO</string>
            <string>System</string>
        </array>
        <key>EnvironmentVariables</key>
        <dict>
            <key>NATS_URL</key>
            <string>{{ nats_server_url }}</string>
            <key>DRIVER_ID</key>
            <string>{{ instance_driver_name }}</string>
            <key>DOMAIN_SERVER_URL</key>
            <string>{{ instance_driver_domain_url }}</string>
        </dict>
        <key>WorkingDirectory</key>
        <string>{{ instance_driver_dir }}</string>
        <key>ProgramArguments</key>
        <array>
            <string>{{ instance_driver_bin }}</string>
            <string>{{ instance_driver_config_file }}</string>
        </array>
        <key>RunAtLoad</key>
        <true />
        <key>KeepAlive</key>
        <dict>
            <key>SuccessfulExit</key>
            <false />
        </dict>
    </dict>
</plist>